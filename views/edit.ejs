<%- include('partials/header', { title: 'Edit' }) %>
<h1>Edit Video</h1>
<form method="POST">
  <label>Title</label>
  <input type="text" name="title" value="<%= video.title %>" required><br/>
  <label>Description</label>
  <textarea name="description" required><%= video.description %></textarea><br/>
  <button type="submit">Update</button>
</form>
<%- include('partials/footer') %>